{
  "name": "@stakekit/widget",
  "version": "0.0.152",
  "type": "module",
  "main": "./dist/package/index.package.js",
  "types": "./dist/types/index.package.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.package.d.ts",
      "default": "./dist/package/index.package.js"
    },
    "./bundle": {
      "types": "./dist/types/index.bundle.d.ts",
      "default": "./dist/bundle/index.bundle.js"
    },
    "./style.css": {
      "default": "./dist/package/style.css"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "!**/.*"
  ],
  "keywords": [
    "stakekit",
    "widget"
  ],
  "author": "Petar Todorovic <petar@omni.app> (https://github.com)",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "vite -c vite/vite.config.dev.ts",
    "build": "yarn turbo run turbo:build",
    "lint": "yarn turbo run turbo:lint",
    "test": "yarn turbo run turbo:test",
    "format": "yarn turbo run turbo:format",
    "turbo:build": "yarn lint && yarn clean && yarn build:package && yarn build:bundle && yarn build:website && yarn build:types",
    "turbo:lint": "tsc && eslint ./src ./tests && prettier --check ./src ./tests",
    "turbo:test": "vitest -c vite/vite.config.dev.ts --retry 3 run",
    "turbo:format": "prettier --write ./src ./tests",
    "build:website": "NODE_OPTIONS=--max-old-space-size=8192 vite -c vite/vite.config.website build",
    "build:package": "vite -c vite/vite.config.package.ts build",
    "build:bundle": "vite -c vite/vite.config.bundle.ts build",
    "build:types": "tsc --project tsconfig.build.json",
    "clean": "rm -rf dist",
    "preview": "vite -c vite/vite.config.dev.ts preview --outDir dist/website",
    "prepare": "husky install",
    "check-unused": "npx knip"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@cassiozen/usestatemachine": "^1.0.1",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@cosmjs/amino": "^0.32.3",
    "@cosmjs/encoding": "0.32.3",
    "@cosmos-kit/core": "2.10.0",
    "@cosmos-kit/keplr": "^2.8.0",
    "@cosmos-kit/leap": "^2.8.0",
    "@cosmos-kit/walletconnect": "2.7.0",
    "@faker-js/faker": "^8.4.1",
    "@ledgerhq/wallet-api-client": "^1.5.8",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@stakekit/api-hooks": "^0.0.68",
    "@stakekit/common": "^0.0.28",
    "@stakekit/rainbowkit": "^2.0.1",
    "@tanstack/react-query": "^5.35.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@tronweb3/tronwallet-abstract-adapter": "^1.1.6",
    "@tronweb3/tronwallet-adapter-bitkeep": "^1.1.1",
    "@tronweb3/tronwallet-adapter-ledger": "^1.1.8",
    "@tronweb3/tronwallet-adapter-tronlink": "^1.1.9",
    "@tronweb3/tronwallet-adapter-walletconnect": "^1.0.6",
    "@types/jsdom": "^21.1.6",
    "@types/lodash.merge": "^4.6.9",
    "@types/react": "18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vanilla-extract/css": "^1.15.1",
    "@vanilla-extract/dynamic": "^2.1.0",
    "@vanilla-extract/recipes": "^0.5.2",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "@vanilla-extract/vite-plugin": "^4.0.9",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "bignumber.js": "^9.1.2",
    "chain-registry": "1.47.1",
    "clsx": "^2.1.1",
    "cosmjs-types": "^0.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eventemitter3": "^5.0.1",
    "framer-motion": "^11.1.9",
    "husky": "^9.0.11",
    "i18next": "^23.11.3",
    "i18next-browser-languagedetector": "^7.2.1",
    "jsdom": "^24.0.0",
    "knip": "^5.13.0",
    "lodash.merge": "^4.6.2",
    "msw": "^2.3.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "purify-ts": "^2.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.1",
    "react-loading-skeleton": "^3.4.0",
    "react-router-dom": "^6.23.0",
    "react-virtuoso": "^4.7.10",
    "reselect": "^5.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^7.8.1",
    "turbo": "^1.13.3",
    "typescript": "5.3.3",
    "unplugin-parcel-macros": "^0.0.3",
    "viem": "^2.10.2",
    "vite": "^5.2.11",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vitest": "^1.6.0",
    "wagmi": "^2.8.3"
  },
  "packageManager": "yarn@4.1.1",
  "resolutions": {
    "@walletconnect/ethereum-provider@npm:2.11.0": "patch:@walletconnect/ethereum-provider@npm%3A2.11.0#~/.yarn/patches/@walletconnect-ethereum-provider-npm-2.11.2-f89f9e67ee.patch",
    "@walletconnect/ethereum-provider@npm:2.13.0": "patch:@walletconnect/ethereum-provider@npm%3A2.13.0#~/.yarn/patches/@walletconnect-ethereum-provider-npm-2.11.2-f89f9e67ee.patch",
    "@walletconnect/sign-client": "2.11.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
