{
  "name": "@stakekit/widget",
  "version": "0.0.20",
  "type": "module",
  "main": "./dist/package/index.package.js",
  "types": "./dist/package/types/src/index.package.d.ts",
  "exports": {
    ".": {
      "import": "./dist/package/index.package.js",
      "types": "./dist/package/types/src/index.package.d.ts"
    },
    "./bundle": {
      "import": "./dist/package/bundle/index.bundle.js",
      "types": "./dist/package/types/src/index.bundle.d.ts"
    },
    "./package/css": {
      "import": "./dist/package/index.package.css"
    },
    "./bundle/css": {
      "import": "./dist/package/bundle/index.bundle.css"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "!**/.*"
  ],
  "keywords": [
    "stakekit",
    "widget"
  ],
  "author": "Petar Todorovic <petar@omni.app> (https://github.com)",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "vite",
    "build": "yarn lint && yarn clean && yarn build:package && yarn build:website",
    "build:website": "NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build:package": "tsx build.ts && yarn build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.json && rm -rf dist/package/types/tests",
    "clean": "rm -rf dist",
    "preview": "vite preview",
    "lint": "tsc && eslint src",
    "test_skip_for_now": "vitest --mode test",
    "test": "exit 0",
    "format": "prettier --write ./src"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17",
    "viem": ">=1.0.0",
    "wagmi": ">=1.0.0"
  },
  "dependencies": {
    "@cassiozen/usestatemachine": "^1.0.1",
    "@cosmjs/amino": "0.31.0",
    "@cosmjs/cosmwasm-stargate": "0.31.0",
    "@cosmjs/encoding": "0.31.0",
    "@cosmjs/stargate": "^0.31.0",
    "@cosmos-kit/core": "2.2.1",
    "@cosmos-kit/keplr": "2.1.6",
    "@cosmos-kit/leap": "2.1.6",
    "@cosmos-kit/react-lite": "2.1.7",
    "@cosmos-kit/walletconnect": "2.1.6",
    "@ledgerhq/iframe-provider": "^0.4.3",
    "@radix-ui/react-alert-dialog": "^1.0.4",
    "@rainbow-me/rainbowkit": "^1.0.8",
    "@stakekit/api-hooks": "^0.0.21",
    "@stakekit/common": "^0.0.13",
    "@tanstack/react-query": "^4.32.6",
    "@vanilla-extract/css": "^1.12.0",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.5.0",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "bignumber.js": "^9.1.1",
    "classnames": "^2.3.2",
    "cosmjs-types": "^0.8.0",
    "eip1193-provider": "^1.0.1",
    "i18next": "^23.4.2",
    "i18next-browser-languagedetector": "^7.1.0",
    "lodash.merge": "^4.6.2",
    "purify-ts": "^2.0.1",
    "react-i18next": "^13.0.3",
    "react-router-dom": "^6.14.2",
    "react-virtuoso": "^4.4.2",
    "reselect": "^4.1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@commitlint/config-lerna-scopes": "^17.6.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/eslint": "^8.44.2",
    "@types/lodash.merge": "^4.6.7",
    "@types/prettier": "^3.0.0",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vanilla-extract/esbuild-plugin": "^2.2.2",
    "@vanilla-extract/vite-plugin": "^3.8.2",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.3.1",
    "esbuild": "^0.18.19",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "msw": "^1.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "viem": "^1.5.3",
    "vite": "^4.4.8",
    "vitest": "^0.34.1",
    "wagmi": "^1.3.9"
  },
  "packageManager": "yarn@3.6.1"
}
