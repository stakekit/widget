{
  "name": "@stakekit/widget",
  "version": "0.0.65",
  "main": "./dist/package/index.package.js",
  "types": "./dist/package/types/src/index.package.d.ts",
  "exports": {
    ".": {
      "import": "./dist/package/index.package.js",
      "types": "./dist/package/types/src/index.package.d.ts"
    },
    "./bundle": {
      "import": "./dist/package/bundle/index.bundle.js",
      "types": "./dist/package/types/src/index.bundle.d.ts"
    },
    "./package/css": {
      "import": "./dist/package/index.package.css"
    },
    "./bundle/css": {
      "import": "./dist/package/bundle/index.bundle.css"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "!**/.*"
  ],
  "keywords": [
    "stakekit",
    "widget"
  ],
  "author": "Petar Todorovic <petar@omni.app> (https://github.com)",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "vite",
    "build": "yarn lint && yarn clean && yarn build:package && yarn build:website",
    "build:website": "NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build:package": "tsx build.ts && yarn build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.json && rm -rf dist/package/types/tests",
    "clean": "rm -rf dist",
    "preview": "vite preview",
    "lint": "tsc && eslint src && prettier --check ./src",
    "test": "exit 0",
    "format": "prettier --write ./src",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17",
    "viem": ">=1.0.0",
    "wagmi": ">=1.0.0"
  },
  "dependencies": {
    "@cassiozen/usestatemachine": "^1.0.1",
    "@cosmjs/cosmwasm-stargate": "0.31.3",
    "@cosmjs/encoding": "0.31.3",
    "@cosmjs/stargate": "^0.31.3",
    "@cosmos-kit/core": "2.7.2",
    "@cosmos-kit/walletconnect": "2.4.3",
    "@ledgerhq/wallet-api-client": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@stakekit/api-hooks": "^0.0.39",
    "@stakekit/common": "^0.0.18",
    "@stakekit/rainbowkit": "^1.0.12",
    "@tanstack/react-query": "^4.36.1",
    "@vanilla-extract/css": "^1.13.0",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/recipes": "^0.5.0",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "bignumber.js": "^9.1.2",
    "clsx": "^2.0.0",
    "cosmjs-types": "^0.9.0",
    "cosmos-kit": "^2.5.4",
    "eip1193-provider": "^1.0.1",
    "i18next": "^23.6.0",
    "i18next-browser-languagedetector": "^7.1.0",
    "lodash.merge": "^4.6.2",
    "purify-ts": "^2.0.1",
    "react-i18next": "^13.3.1",
    "react-loading-skeleton": "^3.3.1",
    "react-router-dom": "^6.17.0",
    "react-virtuoso": "^4.6.2",
    "reselect": "^4.1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.1.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/eslint": "^8.44.6",
    "@types/lodash.merge": "^4.6.8",
    "@types/prettier": "^3.0.0",
    "@types/react": "18.2.32",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vanilla-extract/esbuild-plugin": "^2.3.0",
    "@vanilla-extract/vite-plugin": "^3.9.0",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "autoprefixer": "^10.4.16",
    "babel-plugin-preval": "^5.1.0",
    "chain-registry": "^1.20.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "msw": "^1.3.2",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "tsx": "^3.14.0",
    "turbo": "^1.10.16",
    "typescript": "^5.2.2",
    "viem": "^1.16.6",
    "vite": "^4.5.0",
    "vite-plugin-babel-macros": "^1.0.6",
    "vitest": "^0.34.6",
    "wagmi": "^1.4.5"
  },
  "packageManager": "yarn@4.0.0"
}
