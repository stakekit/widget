{
  "name": "@stakekit/widget",
  "version": "0.0.89",
  "type": "module",
  "main": "./dist/package/index.package.js",
  "types": "./dist/package/types/src/index.package.d.ts",
  "exports": {
    ".": {
      "import": "./dist/package/index.package.js",
      "types": "./dist/package/types/src/index.package.d.ts"
    },
    "./bundle": {
      "import": "./dist/package/bundle/index.bundle.js",
      "types": "./dist/package/types/src/index.bundle.d.ts"
    },
    "./package/css": {
      "import": "./dist/package/index.package.css"
    },
    "./bundle/css": {
      "import": "./dist/package/bundle/index.bundle.css"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "!**/.*"
  ],
  "keywords": [
    "stakekit",
    "widget"
  ],
  "author": "Petar Todorovic <petar@omni.app> (https://github.com)",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "vite",
    "build": "yarn turbo run turbo:build",
    "lint": "yarn turbo run turbo:lint",
    "test": "yarn turbo run turbo:test",
    "format": "yarn turbo run turbo:format",
    "turbo:build": "yarn lint && yarn clean && yarn build:package && yarn build:website",
    "turbo:lint": "tsc && eslint src && prettier --check ./src",
    "turbo:test": "vitest run",
    "turbo:format": "prettier --write ./src",
    "build:website": "NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build:package": "tsx build.ts && yarn build:types",
    "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.json && rm -rf dist/package/types/tests",
    "clean": "rm -rf dist",
    "preview": "vite preview",
    "prepare": "husky install",
    "check-unused": "npx knip",
    "get-errors": "npx node get-errors.js"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "viem": ">=1.0.0",
    "wagmi": ">=1.0.0"
  },
  "dependencies": {
    "@cassiozen/usestatemachine": "^1.0.1",
    "@cosmjs/amino": "^0.32.2",
    "@cosmjs/cosmwasm-stargate": "0.32.2",
    "@cosmjs/encoding": "0.32.2",
    "@cosmjs/stargate": "^0.32.2",
    "@cosmos-kit/core": "2.8.8",
    "@cosmos-kit/keplr": "^2.6.8",
    "@cosmos-kit/leap": "^2.6.8",
    "@cosmos-kit/walletconnect": "2.5.8",
    "@ledgerhq/wallet-api-client": "^1.5.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@stakekit/api-hooks": "^0.0.53",
    "@stakekit/common": "^0.0.24",
    "@stakekit/rainbowkit": "^1.3.8",
    "@tanstack/react-query": "^5.17.19",
    "@tanstack/react-query-devtools": "^5.17.21",
    "@vanilla-extract/css": "^1.14.0",
    "@vanilla-extract/dynamic": "^2.1.0",
    "@vanilla-extract/recipes": "^0.5.1",
    "@vanilla-extract/sprinkles": "^1.6.1",
    "bignumber.js": "^9.1.2",
    "clsx": "^2.1.0",
    "cosmjs-types": "^0.9.0",
    "framer-motion": "^11.0.3",
    "i18next": "^23.7.19",
    "i18next-browser-languagedetector": "^7.2.0",
    "lodash.merge": "^4.6.2",
    "purify-ts": "^2.0.3",
    "react-i18next": "^14.0.1",
    "react-loading-skeleton": "^3.3.1",
    "react-router-dom": "^6.21.3",
    "react-virtuoso": "^4.6.2",
    "reselect": "^5.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.5.0",
    "@commitlint/config-conventional": "^18.5.0",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^8.56.2",
    "@types/lodash.merge": "^4.6.9",
    "@types/react": "18.2.48",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vanilla-extract/esbuild-plugin": "^2.3.2",
    "@vanilla-extract/vite-plugin": "^3.9.4",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "babel-plugin-preval": "^5.1.0",
    "chain-registry": "^1.26.0",
    "dotenv": "^16.4.1",
    "esbuild": "^0.19.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.1",
    "jsdom": "^24.0.0",
    "knip": "^4.2.1",
    "msw": "^2.1.4",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "stream-browserify": "^3.0.0",
    "tsx": "^4.7.0",
    "turbo": "^1.11.3",
    "typescript": "^5.3.3",
    "viem": "^1.21.4",
    "vite": "^5.0.12",
    "vite-plugin-babel-macros": "^1.0.6",
    "vitest": "0.34.2",
    "wagmi": "^1.4.13"
  },
  "packageManager": "yarn@4.0.2",
  "resolutions": {
    "@walletconnect/ethereum-provider@npm:2.11.0": "patch:@walletconnect/ethereum-provider@npm%3A2.11.0#~/.yarn/patches/@walletconnect-ethereum-provider-npm-2.11.0-f89f9e67ee.patch"
  }
}
