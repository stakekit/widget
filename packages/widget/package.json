    "react-i18next": "^16.3.3",
    "react-loading-skeleton": "^3.5.0",
    "react-router": "^7.9.6",
    "reselect": "^5.1.1",
    "rxjs": "^7.8.2",
    "skott": "^0.35.6",
    "tronweb": "^6.1.0",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "unplugin-macros": "^0.18.3",
    "viem": "^2.39.0",
